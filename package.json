{
  "name": "my-facade-library",
  "version": "0.0.4",
  "license": "MIT",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "*"
      ]
    }
  },
  "author": "sarathmc",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sarathmc/ng-my-facade-library.git"
  },
  "keywords": [
    "my-facade-library"
  ],
  "homepage": "https://github.com/sarathmc/ng-my-facade-library",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run copyassets-build && npm run copyfiles && copyfiles -U 1 \"projects/my-lib/src/**/*.js\" build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "link": "ng link",
    "watch": "npm-watch build",
    "e2e": "ng e2e",
    "copyfiles": "copyfiles package.json build",
    "copyassets-build": "copyfiles -U 1 \"projects/my-lib/src/**/*.*\" build",
    "build:clean": "del-cli build",
    "build:link": "npm run build:clean && npm run build"
  },
  "private": false,
  "main": "index.js",
  "module": "index.js",
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "copyfiles": "^2.3.0",
    "npm-watch": "^0.6.0",
    "del-cli": "^3.0.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.3",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
